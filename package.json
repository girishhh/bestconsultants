      "prettier --write"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server/"
    ]
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "exit": true,
    "opts": false,
    "package": "./package.json",
    "reporter": "spec",
    "slow": 75,
    "timeout": false,
    "ui": "bdd",
    "spec": [
      "server/**/*.test.js"
    ],
    "watch-files": [
      "server/**/*.test.js"
    ]
  },
  "nyc": {
    "reporter": [
      "json",
      "text",
      "text-summary",
      "lcov"
    ],
    "all": true,
    "include": [
      "server"
    ],
    "exclude": [
      "**/*.test.js",
      "server/db",
      "server/routes",
      "server/*.js",
      "server/middlewares/Validators",
      "server/coverage"
    ],
    "report-dir": "coverage/backend"
  }
